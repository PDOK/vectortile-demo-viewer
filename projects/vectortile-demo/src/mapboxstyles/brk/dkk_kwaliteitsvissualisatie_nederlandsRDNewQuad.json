{
  "version": 8,
  "name": "",
  "id": "Kadastrale kaart kwaliteitsvisualisatie NetherlandsRDNewQuad",
  "metadata": {
    "ol:webfonts": "https://api.pdok.nl/kadaster/kadastralekaart/ogc/v1-demo/resources/fonts/{font-family}/{fontweight}{-fontstyle}.css",
    "gokoala:title-items": "id"
  },
  "center": [],
  "pitch": 0,
  "sources": {
    "dkk": {
      "type": "vector",
      "tiles": [
        "https://api.pdok.nl/kadaster/kadastralekaart/ogc/v1-demo/tiles/NetherlandsRDNewQuad/{z}/{y}/{x}?f=mvt"
      ],
      "minzoom": 12,
      "maxzoom": 12
    }
  },
  "glyphs": "https://api.pdok.nl/kadaster/kadastralekaart/ogc/v1-demo/resources/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "Bebouwing",
      "type": "fill",
      "source": "dkk",
      "source-layer": "pand",
      "paint": { "fill-color": "#E6E6E6", "fill-outline-color": "#CDCDCD" }
    },
    {
      "id": "Kwaliteitslabel Onbekend",
      "type": "line",
      "source": "dkk",
      "source-layer": "kadastrale_grens",
      "filter": ["in", "type_grens_waarde", "Voorlopig", "Administratief"],
      "layout": { "line-join": "round" },
      "paint": {
        "line-dasharray": [4, 2],
        "line-color": "#bbbbbb",
        "line-width": 1.5
      }
    },
    {
      "id": "Kwaliteitslabel E",
      "type": "line",
      "source": "dkk",
      "source-layer": "kadastrale_grens",
      "filter": ["==", "classificatie_kwaliteit_waarde", "E"],
      "paint": { "line-color": "#fe9c33", "line-width": 1.5 }
    },
    {
      "id": "Kwaliteitslabel D",
      "type": "line",
      "source": "dkk",
      "source-layer": "kadastrale_grens",
      "filter": ["==", "classificatie_kwaliteit_waarde", "D"],
      "paint": { "line-color": "#fed98e", "line-width": 1.5 }
    },
    {
      "id": "Kwaliteitslabel C",
      "type": "line",
      "source": "dkk",
      "source-layer": "kadastrale_grens",
      "filter": ["==", "classificatie_kwaliteit_waarde", "C"],
      "paint": { "line-color": "#bae4bc", "line-width": 1.5 }
    },
    {
      "id": "Kwaliteitslabel B",
      "type": "line",
      "source": "dkk",
      "source-layer": "kadastrale_grens",
      "filter": ["==", "classificatie_kwaliteit_waarde", "B"],
      "paint": { "line-color": "#008200", "line-width": 1.5 }
    },
    {
      "id": "Bijpijling",
      "type": "line",
      "source": "dkk",
      "source-layer": "perceel_bijpijling",
      "paint": { "line-color": "#000001", "line-width": 1 }
    },
    {
      "id": "Nummeraanduidingreeks",
      "type": "symbol",
      "source": "dkk",
      "source-layer": "pand_nummeraanduiding",
      "layout": {
        "text-field": "{tekst}",
        "text-size": [
      "step",
      ["zoom"],
      6,
            12, 6,
            13, 7,
            14, 8,
            15, 11,
            16, 13
          ]
        ,
        "text-rotate": ["get", "hoek"],
        "text-font": [
          "Liberation Sans",
          "system-ui",
          "Roboto",
          "Arial",
          "Noto Sans",
          "Liberation Sans",
          "sans-serif",
          "Noto Color Emoji"
        ],
        "text-allow-overlap": true,
        "icon-allow-overlap": true,
        "symbol-placement": "line-center"
      },
      "paint": { "text-color": "#797979" }
    },
    {
      "id": "Water openbareruimtelabel",
      "type": "symbol",
      "source": "dkk",
      "source-layer": "openbareruimtelabel",
      "paint": {
        "text-color": "#000000"
      },
      "filter": [
        "all",
        ["==", "openbare_ruimte_type", "Water"],
        ["==", "bgt_status", "bestaand"]
      ],
      "layout": {
        "text-field": "{tekst}",
        "text-font": [
          "Liberation Sans Italic",
          "system-ui",
          "Roboto",
          "Arial",
          "Noto Sans",
          "Liberation Sans",
          "sans-serif",
          "Noto Color Emoji"
        ],
          "text-size": [
      "step",
      ["zoom"],
      11,
            12, 11,
            13, 11,
            14, 12,
            15, 14
          ],
        "text-rotate": ["-", 360, ["get", "mshoek"]]
      }
    },
    {
      "id": "Weg openbareruimtelabel",
      "type": "symbol",
      "source": "dkk",
      "source-layer": "openbareruimtelabel",
      "paint": {
        "text-color": "#000000"
      },
      "filter": [
        "all",
        ["==", "openbare_ruimte_type", "Weg"],
        ["==", "bgt_status", "bestaand"]
      ],
      "layout": {
        "text-field": "{tekst}",
        "text-font": [
          "Liberation Sans",
          "system-ui",
          "Roboto",
          "Arial",
          "Noto Sans",
          "Liberation Sans",
          "sans-serif",
          "Noto Color Emoji"
        ],
         "text-size": [
      "step",
      ["zoom"],
      11,
            12, 11,
            13, 11,
            14, 12,
            15, 14
          ],
        "text-rotate": ["-", 360, ["get", "mshoek"]]
      }
    },
    {
      "id": "Perceel label",
      "type": "symbol",
      "source": "dkk",
      "source-layer": "perceel_label",
      "layout": {
        "text-field": "{perceelnummer}",
        "text-allow-overlap": true,
        "icon-allow-overlap": true,
        "text-size": [
      "step",
      ["zoom"],
      9,
            12, 9,
            13, 9,
            14, 12,
            15, 14,
            16, 15
          ],
        "text-font": [
          "Liberation Sans Italic",
          "system-ui",
          "Roboto",
          "Arial",
          "Noto Sans",
          "Liberation Sans",
          "sans-serif",
          "Noto Color Emoji"
        ]
      },
      "paint": {
        "text-color": "#000000",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": [
      "step",
      ["zoom"],
      10,
            12, 10,
            13, 10,
            14, 13,
            15, 15,
            16, 16
          ]
      }
    }
  ]
}
