{
  "name": "vectortile-demo-viewer",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=127.0.0.1",
    "build": "npm run val && run ngtestci && ng build",
    "watch": "ng build --watch --configuration development",
    "ngtest": "ng test",
    "ngtestci": "ng test  --browsers=ChromeHeadless --watch=false",
    "deploy": "ng deploy --dir=dist/vectortile-demo/browser  --base-href=https://pdok.github.io/vectortile-demo-viewer/ --repo=https://github.com/pdok/vectortile-demo-viewer.git --name=JLSchaap --email=John.Schaap@Kadaster.nl",
    "e2e": "ng e2e",
    "open": "cypress open",
    "testng": "cypress run",
    "lint": "ng lint --fix",
    "val": "npm run valbrk  && npm run valachter && npm run valstandaard && npm run valtactiel",
    "valbag": "gl-style-validate projects/vectortile-demo/src/mapboxstyles/bagstd.json",
    "valachter": "gl-style-validate projects/vectortile-demo/src/mapboxstyles/bgt_achtergrondvisualisatie.json",
    "valstandaard": "gl-style-validate projects/vectortile-demo/src/mapboxstyles/bgt_standaardvisualisatie.json",
    "valtactiel": "gl-style-validate projects/vectortile-demo/src/mapboxstyles/tactielevisualisatie.json",
    "valbrk": "gl-style-validate projects/vectortile-demo/src/mapboxstyles/brk/*.json",
    "valbrkformat": "gl-style-format projects/vectortile-demo/src/mapboxstyles/brk/dkk_kwaliteitsvissualisatie_webmercatorquad.json > projects/vectortile-demo/src/mapboxstyles/brk/dkk_kwaliteitsvissualisatie_webmercatorquad2json ",
    "valbrt": "gl-style-validate projects/vectortile-demo/src/mapboxstyles/brt/*.json",
    "valbrtAFormat": "gl-style-format projects/vectortile-demo/src/mapboxstyles/brt/brt_achtergrondkaart_standaard_nederlandsRDNewQuad.json > projects/vectortile-demo/src/mapboxstyles/brt/brt_achtergrondkaart_standaard_nederlandsRDNewQuad_Formatted.json",
    "runupdate": "ng update  @angular/core @angular/cli @angular/cdk @angular/material"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.5",
    "@angular/cdk": "^19.2.8",
    "@angular/common": "^19.2.5",
    "@angular/compiler": "^19.2.5",
    "@angular/core": "^19.2.5",
    "@angular/elements": "^19.2.5",
    "@angular/forms": "^19.2.5",
    "@angular/material": "^19.2.8",
    "@angular/platform-browser": "^19.2.5",
    "@angular/platform-browser-dynamic": "^19.2.5",
    "@angular/router": "^19.2.5",
    "@mapbox/mapbox-gl-style-spec": "^14.11.0",
    "@types/geojson": "^7946.0.16",
    "ngx-color-picker": "^16.0.0",
    "ol": "9.2.4",
    "ol-mapbox-style": "12.6.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.6",
    "@angular-eslint/builder": "19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/schematics": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/cli": "^19.2.6",
    "@angular/compiler-cli": "^19.2.5",
    "@types/jasmine": "~4.3.5",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "angular-cli-ghpages": "^2.0.3",
    "angular-eslint": "^19.3.0",
    "eslint": "9.24.0",
    "gh-pages": "^6.3.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.3"
  }
}
